{
  "name": "images-cloud-server",
  "version": "1.0.0",
  "description": "Server for images-cloud app",
  "main": "server.js",
  "scripts": {
    "build": "concurrently \"npx tsc\" \"npm run copy\"",
    "start": "node dist/server.js",
    "dev": "npx tsc-watch --onSuccess \"concurrently 'npm run copy' 'nodemon -q dist/server.js'\"",
    "test": "jest",
    "copy": "copyfiles -e \"./src/**/*.ts\" \"./src/**\" \"./dist\""
  },
  "author": "Jakub Korytko",
  "license": "ISC",
  "dependencies": {
    "@types/multer": "^1.4.7",
    "better-sqlite3": "^7.5.3",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.17.2",
    "image-size": "^1.0.1",
    "jest": "^28.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.4",
    "sharp": "^0.30.6",
    "tsc-watch": "^5.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/node": "^17.0.40",
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.19",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3"
  }
}
