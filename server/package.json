{
  "name": "images-cloud-server",
  "version": "0.1.0",
  "description": "Server for images-cloud app",
  "keywords": [
    "open-source",
    "jwt",
    "image-gallery",
    "mit-license",
    "file-hosting"
  ],
  "license": "MIT",
  "author": "Jakub Korytko",
  "scripts": {
    "build": "run-s tsc copy",
    "start": "nodemon -q dist/server.js",
    "watch": "npx tsc-watch --onSuccess \"npm start\"",
    "tsc": "npx tsc",
    "dev": "run-s copy watch",
    "test": "jest",
    "copy": "copyfiles -e \"./src/**/*.ts\" \"./src/**\" \"./dist\"",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx src"
  },
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.17.2",
    "image-size": "^1.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.4",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "15.0.0",
    "eslint-plugin-promise": "6.0.0",
    "jest": "^28.1.1",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "sort-package-json": "^2.6.0",
    "ts-jest": "^28.0.5",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.3"
  }
}
